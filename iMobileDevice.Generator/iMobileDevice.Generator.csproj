<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <Platforms>x64</Platforms>
    <AssetTargetFallback>$(AssetTargetFallback);native</AssetTargetFallback>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup Condition="'$(TargetFramework)' != 'native'">
    <PackageReference Include="Core.Clang" Version="5.1.5-beta" />
    <PackageReference Include="System.CodeDom" Version="4.4.0" />
    <PackageReference Include="Native.LibClang" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

  <Import Project="../native.props" />

  <ItemGroup>
    <PackageReference Include="ideviceinstaller" Version="$(ideviceinstaller)" />
    <PackageReference Include="ideviceinstaller.redist" Version="$(ideviceinstaller)" />
    <PackageReference Include="ideviceinstaller.symbols" Version="$(ideviceinstaller)" />
    <PackageReference Include="libzip" Version="$(libzip)" />
    <PackageReference Include="libzip.redist" Version="$(libzip)" />
    <PackageReference Include="libimobiledevice" Version="$(libimobiledevice)" />
    <PackageReference Include="libimobiledevice.redist" Version="$(libimobiledevice)" />
    <PackageReference Include="libimobiledevice.symbols" Version="$(libimobiledevice)" />
    <PackageReference Include="libplist" Version="$(libplist)" />
    <PackageReference Include="libplist.symbols" Version="$(libplist)" />
    <PackageReference Include="libusbmuxd" Version="$(libusbmuxd)" />
    <PackageReference Include="libusbmuxd.redist" Version="$(libusbmuxd)" />
    <PackageReference Include="libusbmuxd.symbols" Version="$(libusbmuxd)" />
    <PackageReference Include="libideviceactivation" Version="$(libideviceactivation)" />
    <PackageReference Include="libideviceactivation.redist" Version="$(libideviceactivation)" />
    <PackageReference Include="libideviceactivation.symbols" Version="$(libideviceactivation)" />
    <PackageReference Include="usbmuxd" Version="$(usbmuxd)" />
    <PackageReference Include="libiconv" Version="$(libiconv)" />
    <PackageReference Include="fix8.dependencies.getopt" Version="$(getopt)" />
    <PackageReference Include="libiconv.redist" Version="$(libiconv)" />
    <PackageReference Include="pthreads" Version="$(pthreads)" />
    <PackageReference Include="pthreads.redist" Version="$(pthreads)" />
    <PackageReference Include="openssl.v140.windesktop.msvcstl.dyn.rt-dyn" Version="$(openssl)" />
    <PackageReference Include="openssl.v140.windesktop.msvcstl.dyn.rt-dyn.x64" Version="$(openssl)" />
    <PackageReference Include="openssl.v140.windesktop.msvcstl.dyn.rt-dyn.x86" Version="$(openssl)" />
    <PackageReference Include="zlib.v140.windesktop.msvcstl.dyn.rt-dyn" Version="$(zlib)" />
    <PackageReference Include="curl" Version="$(curl)" />
    <PackageReference Include="curl.redist" Version="$(curl)" />
    <PackageReference Include="libxml2" Version="$(libxml2)" />
    <PackageReference Include="libxml2.redist" Version="$(libxml2)" />
    <PackageReference Include="libssh2" Version="$(libssh2)" />
    <PackageReference Include="libssh2.redist" Version="$(libssh2)" />
  </ItemGroup>
</Project>
