    <Folder Include="Properties\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="ideviceinstaller" Version="1.1.0.19" />
    <PackageReference Include="ideviceinstaller.redist" Version="1.1.0.19" />
    <PackageReference Include="ideviceinstaller.symbols" Version="1.1.0.19" />
    <PackageReference Include="libzip" Version="1.1.2.7" />
    <PackageReference Include="libzip.redist" Version="1.1.2.7" />
    <PackageReference Include="libimobiledevice" Version="1.2.1.196" />
    <PackageReference Include="libimobiledevice.redist" Version="1.2.1.196" />
    <PackageReference Include="libimobiledevice.symbols" Version="1.2.1.196" />
    <PackageReference Include="libplist" Version="2.0.1.171" />
    <PackageReference Include="libplist.symbols" Version="2.0.1.171" />
    <PackageReference Include="libusbmuxd" Version="1.0.10.92" />
    <PackageReference Include="libusbmuxd.redist" Version="1.0.10.92" />
    <PackageReference Include="libusbmuxd.symbols" Version="1.0.10.92" />
    <PackageReference Include="libideviceactivation" Version="1.0.0.23" />
    <PackageReference Include="libideviceactivation.redist" Version="1.0.0.23" />
